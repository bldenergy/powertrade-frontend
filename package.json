{
  "name": "mvp-frontend",
  "version": "0.0.1",
  "config": {
    "prettierTarget": "**/*.{tsx,ts,json,md,js,css}"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "build:lib": "rm -rf dist && rollup -c",
    "prepublishOnly": "npm run build:lib",
    "test": "cypress run",
    "test:dev": "cypress open"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@ory/client": "0.0.1-alpha.169",
    "@ory/hydra-client": "1.11.7",
    "@ory/integrations": "^0.2.5",
    "@ory/kratos-client": "0.9.0-alpha.3",
    "chart.js": "^3.7.1",
    "classnames": "^2.3.1",
    "csurf": "^1.11.0",
    "express": "^4.18.0",
    "next": "^12.1.5",
    "openid-client": "5.1.5",
    "react": "^18.1.0",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.1.0",
    "react-toastify": "^8.2.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/react": "~18.0.8",
    "@types/request": "^2.48.8",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^9.6.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "ory-prettier-styles": "^1.1.2",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
