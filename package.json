{
  "name": "mvp-frontend",
  "version": "0.0.1",
  "config": {
    "prettierTarget": "**/*.{tsx,ts,json,md,js,css}"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "build:lib": "rm -rf dist && rollup -c",
    "prepublishOnly": "npm run build:lib",
    "test": "cypress run",
    "test:dev": "cypress open"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "chart.js": "^3.8.0",
    "chartjs-adapter-luxon": "^1.1.0",
    "chartjs-plugin-streaming": "^2.0.0",
    "chartjs-plugin-zoom": "^1.2.1",
    "express": "^4.18.1",
    "firebase": "^9.8.4",
    "framer-motion": "^6.3.16",
    "google-protobuf": "3.20.1",
    "grpc-web": "1.3.1",
    "localforage": "^1.10.0",
    "next": "^12.2.0",
    "next-auth": "^4.10.3",
    "next-pwa": "^5.5.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/react": "~18.0.14",
    "@types/request": "^2.48.8",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.3.0",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "ory-prettier-styles": "^1.3.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
